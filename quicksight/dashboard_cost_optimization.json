{
  "name": "Kiro成本优化",
  "description": "超额分析、Credit 利用率、用户消耗明细",
  "data_source": "user_report (credits 数据集)",
  "visuals": [
    {"type": "KPI", "title": "有超额的用户数", "metric": "DISTINCT_COUNT(userid WHERE overage_credits_used > 0)"},
    {"type": "KPI", "title": "总超额 Credit", "metric": "SUM(overage_credits_used)"},
    {"type": "KPI", "title": "平均每用户 Credit", "metric": "AVG(credits_used)"},
    {"type": "LineChart", "title": "每日超额趋势", "x": "date", "y": "SUM(overage_credits_used)"},
    {"type": "BarChart", "title": "各层级平均 Credit 消耗", "x": "subscription_tier", "y": ["AVG(credits_used)", "AVG(overage_cap)"]},
    {"type": "Table", "title": "用户 Credit 使用明细", "columns": ["userid", "subscription_tier", "client_type", "SUM(credits_used)", "SUM(overage_credits_used)", "MAX(overage_cap)", "SUM(total_messages)"]}
  ]
}
