{
  "name": "Kiro用户行为分析",
  "description": "代码生成、Inline 接受率、用户活跃度",
  "data_source": "by_user_analytic (行为数据集)",
  "visuals": [
    {"type": "KPI", "title": "总 AI 代码行数", "metric": "SUM(chat_aicodelines)"},
    {"type": "KPI", "title": "总 Inline 代码行数", "metric": "SUM(inline_aicodelines)"},
    {"type": "KPI", "title": "总 Chat 消息数", "metric": "SUM(chat_messagessent)"},
    {"type": "LineChart", "title": "每日 AI 代码生成趋势", "x": "date", "y": ["SUM(chat_aicodelines)", "SUM(inline_aicodelines)"]},
    {"type": "LineChart", "title": "Inline 代码接受趋势", "x": "date", "y": ["SUM(inline_acceptancecount)", "SUM(inline_suggestionscount)"]},
    {"type": "BarChart", "title": "Top 10 代码生成用户", "x": "userid", "y": ["SUM(chat_aicodelines)", "SUM(inline_aicodelines)"], "limit": 10},
    {"type": "Table", "title": "用户行为明细", "columns": ["userid", "SUM(chat_messagessent)", "SUM(chat_aicodelines)", "SUM(inline_aicodelines)", "SUM(inline_acceptancecount)", "SUM(inline_suggestionscount)"]}
  ]
}
